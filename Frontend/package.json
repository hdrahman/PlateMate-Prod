{
  "name": "platemate",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"--no-experimental-fetch --dns-result-order=ipv4first\" EXPO_CLI_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --offline",
    "start-legacy": "cross-env EXPO_CLI_NO_DOCTOR=1 expo start",
    "start-online": "cross-env NODE_OPTIONS=\"--no-experimental-fetch\" EXPO_CLI_NO_DOCTOR=1 expo start",
    "start-dev": "expo start",
    "start-notifee": "cross-env NODE_OPTIONS=\"--no-experimental-fetch\" EXPO_CLI_NO_DOCTOR=1 expo start --dev-client",
    "build-dev": "eas build --profile development --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/config-plugins": "~54.0.1",
    "@expo/prebuild-config": "~54.0.0",
    "@expo/vector-icons": "^15.0.2",
    "@notifee/react-native": "^9.1.8",
    "@quidone/react-native-wheel-picker": "^1.4.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.2",
    "@supabase/supabase-js": "^2.50.0",
    "@types/base-64": "^1.0.2",
    "axios": "^1.7.9",
    "base-64": "^1.0.0",
    "buffer": "^6.0.3",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "eas-cli": "^16.3.3",
    "events": "^3.3.0",
    "expo": "^54.0.0",
    "expo-asset": "~12.0.9",
    "expo-av": "~16.0.7",
    "expo-background-fetch": "~14.0.7",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.15",
    "expo-device": "~8.0.9",
    "expo-eas-client": "^1.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-notifications": "~0.32.12",
    "expo-sensors": "~15.0.7",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.7",
    "expo-updates": "^29.0.12",
    "googleapis": "^148.0.0",
    "lottie-react-native": "~7.3.1",
    "lucide-react": "^0.475.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-background-actions": "^4.0.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-circular-progress": "^1.4.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-health": "^1.19.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.13.1",
    "react-native-purchases": "^8.11.9",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-vision-camera": "^4.7.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^19.1.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~19.1.0",
    "cross-env": "^7.0.3",
    "expo": "~54.0.0",
    "expo-dev-client": "~6.0.15",
    "jest": "^29.7.0",
    "msw": "^2.7.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "__tests__/jest.setup.js",
      "@testing-library/jest-native/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|expo|@expo|react-navigation|@react-navigation|react-native-vector-icons|react-native-chart-kit)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts",
      "!src/index.js"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "expo-barcode-scanner"
        ],
        "listUnknownPackages": false
      },
      "checkNpmPackages": false,
      "checkExpoSdkVersions": false,
      "checkVirtualizedLists": false,
      "checkMissingDependencies": false,
      "checkDependencyVersions": false,
      "checkNativeModules": false
    },
    "install": {
      "exclude": [
        "react-native@~0.81.0"
      ]
    }
  }
}
