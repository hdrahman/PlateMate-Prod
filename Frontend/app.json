{
  "expo": {
    "name": "PlateMate",
    "slug": "PlateMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "sdkVersion": "53.0.0",
    "splash": {
      "image": "./assets/icon2 - Edited.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.platemate.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.platemate.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      "expo-sqlite",
      "./notifee.plugin.js"
    ],
    "extra": {
      "eas": {
        "projectId": "6c1ede6a-d918-4fbc-a5b8-c549ed290ff6"
      }
    },
    "doctor": {
      "checkNpmPackages": false,
      "checkExpoSdkVersions": false,
      "checkVirtualizedLists": false,
      "checkMissingDependencies": false,
      "checkDependencyVersions": false,
      "checkNativeModules": false
    },
    "owner": "haamed"
  }
}